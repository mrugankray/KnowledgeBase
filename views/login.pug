extends layout

block content
    h1 #{Register}
    form(method = 'POST',action="/users/login")
        #form-group
            label Username:
            input.form-control(type = 'text',name = 'username')
        #form-group
            label Password:
            input.form-control(type = 'password',name = 'password')
        input.btn.btn-primray(type='submit',value="Submit")